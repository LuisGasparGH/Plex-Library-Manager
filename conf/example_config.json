{
    "plmData":{
        "logger":{
            "name": "plm-logger",
            "path": "/logs/plm/plex-library-manager.log"
        }   
    },
    "dbData":{
        "logger":{
            "name": "db-logger",
            "path": "/logs/db/db-manager.log"

        },
        "url": "mongodb://localhost:27017/",
        "name": "plexDatabase",
        "collections":{
            "tv": "tvCollection",
            "movies": "moviesCollection"
        }
    },
    "fileData":{
        "logger":{
            "name": "file-logger",
            "path": "/logs/file/file-manager.log"
        },
        "path":{
            "tv": "/path/to/plex/Library/Shows/",
            "movies": "/path/to/plex/Library/Movies/"
        }
    },
    "plexData":{
        "logger":{
            "name": "plex-logger",
            "path": "/logs/plex/plex-manager.log"
        } ,
        "username": "username",
        "password": "password",
        "servername": "servername"
    },
    "qbtData":{
        "logger":{
            "name": "qbt-logger",
            "path": "/logs/qbt/qbt-manager.log"
        },
        "username": "username",
        "password": "password",
        "port": 8080,
        "supported_extensions": [".mkv",".mp4",".avi", ".mov"]
    },
    "rarbgData":{
        "logger":{
            "name": "rarbg-logger",
            "path": "/logs/rarbg/rarbg-manager.log"
        },
        "retries": 10,
        "categories":{
            "tv": "tv_category_of_choice",
            "movies": "movies_category_of_choice"
        }
    },
    "tmdbData":{
        "logger":{
            "name": "tmdb-logger",
            "path": "/logs/tmdb/tmdb-manager.log"
        },
        "apiKey": "yourAPIkey",
        "lists":{
            "tv": "tv_list_id",
            "movies": "movies_list_id"
        }
    }
}
